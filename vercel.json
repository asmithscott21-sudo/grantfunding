{
  "name": "GrantFunding",
  "version": 2,
  "buildCommand": "bun run build",
  "outputDirectory": ".next/standalone",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "build": {
    "env": {
      "NEXT_PUBLIC_SITE_URL": "https://grantfunding.vercel.app"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
